//relational operators
#include<stdio.h>
int main(void)
{
    int a,b,c;
    printf("enter the values");
     scanf("%d %d %d",&a,&b, &c);
    printf("%d==%dis %d\n",a,c,a==c);
    printf("%d>%d is %d\n",a,b,a>b);
    printf("%d<%d is %d\n",a,b,a<b);
    printf("%d>%d is %d\n" ,a,c,a>c);
     printf("%d<%d is %d\n" ,a,c,a<c);
      printf("%d!=%d is %d\n" ,a,b,a!=b);
      printf("%d!=%d is %d\n" ,a,c,a!=c);
      printf("%d>=%d is %d\n" ,a,c,a>=c);
       printf("%d<=%d is %d\n" ,a,c,a<=c);
       printf("%d>=%d is %d\n" ,a,b,a>=b);

       return 0;
}
